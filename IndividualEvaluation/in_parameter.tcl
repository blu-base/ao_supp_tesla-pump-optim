## Data collection to be substituted for each individual evaluation
## 
## Do not forget the dot to mark a float
set dsksp 0.5 		# mm      # gap between disks
set dskrdout 25.0	# mm      # outer radius of impeller
set dskrdin 5.0		# mm      # inner radius of impeller
set diffratio 3.0	# -       # diffusor outlet/inlet size ratio
set diffang 8.0		# degree  # diffusor angle
set dskth 0.5		# mm      # disc thickness
set volsp 1.5		# mm	  # tongue-to-impeller gap
set tonguerd 0.05	# mm      # tongue radius
set volConstructor 0.1	# -   # volute constructor parameter
#
set nglob 0.3       # -       # Globa mesh density scaling factor
